void swap(void *a, void *b, const size_t nmemb) {
    size_t idx = 0;

    while (idx + 8 < nmemb) {
        long long c = *((long long *) a + idx);
        *((long long *) a + idx) = *((long long *) b + idx);
        *((long long *) b + idx) = c;
    }
    while (nmemb) {
        while (nmemb > 8) {
            size_t i = 0;
            for (size_t i = 0; i < nmemb; i++) {

            }
        }
    }

}